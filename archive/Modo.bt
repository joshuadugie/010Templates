    } FILESPEC <read=fn_Read_Filespec>;
    local int i;
    for( i=0; i<refscount; i++ )
        FILESPEC  ref;
    local int subchunks = FTell();
    while( FTell() - pos < size )
        fn_Read_REFS_Subchunks(subchunks);
